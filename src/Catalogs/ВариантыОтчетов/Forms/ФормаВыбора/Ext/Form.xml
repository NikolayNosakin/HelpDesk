	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	ВариантыОтчетов.Ссылка,
	ВариантыОтчетов.ВерсияДанных,
	ВариантыОтчетов.ПометкаУдаления,
	ВариантыОтчетов.Предопределенный,
	ВариантыОтчетов.Наименование,
	ВариантыОтчетов.КлючОбъекта,
	ВариантыОтчетов.Администратор,
	ВЫРАЗИТЬ(ВариантыОтчетов.Описание КАК СТРОКА(1000)) КАК Описание,
	ВариантыОтчетов.ТипВариантаОтчета,
	ВариантыОтчетов.КлючВарианта,
	ВариантыОтчетов.БыстрыйДоступ,
	ВариантыОтчетов.ПредставлениеОбъекта
ИЗ
	Справочник.ВариантыОтчетов КАК ВариантыОтчетов
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ВариантыОтчетов.ИсключенияБыстрогоДоступа КАК ВариантыОтчетовИсключенияБыстрогоДоступа
		ПО ВариантыОтчетов.Ссылка = ВариантыОтчетовИсключенияБыстрогоДоступа.Ссылка
			И (ВариантыОтчетовИсключенияБыстрогоДоступа.Пользователь = &amp;ТекущийПользователь)
ГДЕ
	(&amp;Подсистема = ""
			ИЛИ ВариантыОтчетов.Подсистемы.Подсистема ПОДОБНО &amp;Подсистема + "%")
	И ВариантыОтчетов.КлючОбъекта В(&amp;СписокОтчетов)
	И ВариантыОтчетов.ТипВариантаОтчета В(&amp;СписокТипов)
	И ВЫБОР
			КОГДА ВариантыОтчетовИсключенияБыстрогоДоступа.Пользователь ЕСТЬ NULL 
				ТОГДА ВариантыОтчетов.БыстрыйДоступ = ИСТИНА
			ИНАЧЕ ВариантыОтчетов.БыстрыйДоступ = ЛОЖЬ
		КОНЕЦ</QueryText>
				<MainTable>Catalog.ВариантыОтчетов</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
